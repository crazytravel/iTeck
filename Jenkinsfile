pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'node build'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing...'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying...'
            }
        }
        stage('Start') {
            steps {
                echo 'node start'
            }
        }
    }
}